{
  "name": "redux-views",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "github:josepot/redux-views",
  "bugs": "https://github.com/josepot/redux-views/issues",
  "keywords": [
    "redux",
    "selectors",
    "cached",
    "functional"
  ],
  "author": "Josep M Sobrepere (https://github.com/josepot)",
  "main": "lib/redux-views.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/redux-views.js",
  "module": "es/redux-views.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types/index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test"
  },
  "dependencies": {
    "loose-envify": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.0.0",
    "prettier": "^1.16.1",
    "ramda": "^0.26.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.3"
  },
  "npmName": "redux-views",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$",
    "testURL": "http://localhost/"
  },
  "sideEffects": false
}
